<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tareq.kuetian">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"></uses-permission>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.tareq.kuetian.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.tareq.kuetian.Sign_in_activity"
            android:label="KUETian"></activity>
        <activity
            android:name="com.tareq.kuetian.ManageBatchView"
            android:label="KUETian"></activity>
        <activity
            android:name="com.tareq.kuetian.Sign_up_activity"
            android:label="KUETian"></activity>
        <activity
            android:name="com.tareq.kuetian.ManageDeptView"
            android:label="KUETian"></activity>
        <activity
            android:name="com.tareq.kuetian.ManageRollView"
            android:label="KUETian"></activity>
        <activity
            android:name="com.tareq.kuetian.EditInfoView"
            android:label="KUETian"></activity>
        <activity android:name=".PhoneVarifyAcitivity" />
    </application>

</manifest>